{
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "jquery": true,
      "jest": true,
      "es6": true,
      "node": true,
      "amd": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "comma-dangle": ["error", "only-multiline"]
    },
    "rules": {
      "semi": "error"
    }
  }
  